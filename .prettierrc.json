{
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindConfig": "./docs/tailwind.config.js",
  "semi": false,
  "singleQuote": true,
  "printWidth": 200,
  "trailingComma": "es5",
  "arrowParens": "always",
  "tabWidth": 2,
  "useTabs": false,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "strict"
}
